{
  "name": "approuter",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel start:approuter start:ui5-app start:browser",
    "start:approuter": "node -r dotenv/config node_modules/@sap/approuter/approuter.js",
    "start:ui5-app": "pnpm --filter ui5-app dev:no-open",
    "start:browser": "wait-port http://localhost:1081 && open-cli http://localhost:5000"
  },
  "dependencies": {
    "@sap/approuter": "^14.1.1",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.2.0",
    "wait-port": "^1.0.4"
  }
}
